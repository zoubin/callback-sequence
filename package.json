{
  "name": "callback-sequence",
  "version": "1.3.2",
  "description": "Make a new callback to run input callbacks in sequence",
  "main": "index.js",
  "scripts": {
    "test": "npm run lint && task-tape test/*.js | tap-summary",
    "lint": "eslint *.js lib/**/*.js test/*.js bin/*.js",
    "coverage": "istanbul cover -i 'lib/**/*.js' -i '*.js' --print both task-tape -- test/*.js",
    "check-coverage": "istanbul check-coverage --statements 90 --functions 90 --branches 85 --lines 90",
    "upload-coverage": "cat ./coverage/lcov.info | coveralls",
    "cover": "npm run coverage && npm run check-coverage && npm run upload-coverage"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/zoubin/callback-sequence.git"
  },
  "keywords": [
    "gulp",
    "task",
    "sequence",
    "callback",
    "async"
  ],
  "author": "zoubin",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/zoubin/callback-sequence/issues"
  },
  "homepage": "https://github.com/zoubin/callback-sequence#readme",
  "dependencies": {
    "run-callback": "^2.0.0"
  },
  "devDependencies": {
    "coveralls": "^2.11.4",
    "eslint": "^1.10.1",
    "istanbul": "^0.4.0",
    "tap-summary": "^1.0.0",
    "tape": "^4.2.0",
    "task-tape": "^1.0.0"
  }
}
